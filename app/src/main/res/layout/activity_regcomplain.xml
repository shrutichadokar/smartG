<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".regcomplain">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="302dp"
        android:layout_height="69dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="61dp"
        android:layout_marginBottom="624dp"
        android:text="Register Complain"
        android:textColor="#0DF685"
        android:textSize="34sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView_show_name0"
        android:layout_width="385dp"
        android:layout_height="48dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="554dp"
        android:autofillHints=""
        android:ems="10"
        android:hint="Name"
        android:inputType="textPersonName"
        android:textColor="#0C0A0A"
        android:textSize="20sp"
        tools:ignore="HardcodedText,TextViewEdits" />

    <TextView
        android:id="@+id/textView_show_email"
        android:layout_width="397dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="6dp"
        android:layout_marginBottom="487dp"
        android:autofillHints=""
        android:ems="10"
        android:hint="Email"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:textColor="#040303"
        android:textSize="20sp"
        tools:ignore="HardcodedText,TextViewEdits" />

    <TextView
        android:id="@+id/textView_show_mobileno0"
        android:layout_width="395dp"
        android:layout_height="55dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="417dp"
        android:autofillHints=""
        android:ems="10"
        android:hint="Mobile no"
        android:inputType="textPersonName"
        android:textColor="#090808"
        android:textSize="20sp"
        tools:ignore="HardcodedText,TextViewEdits" />

    <TextView
        android:id="@+id/textView_show_address"
        android:layout_width="386dp"
        android:layout_height="53dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="345dp"
        android:autofillHints=""
        android:ems="10"
        android:hint="Address"
        android:inputType="textPersonName"
        android:textColor="#090909"
        android:textSize="20sp"
        tools:ignore="HardcodedText,TextViewEdits" />

    <Button
        android:id="@+id/button11"
        android:layout_width="wrap_content"
        android:layout_height="62dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="70dp"
        android:layout_marginBottom="110dp"
        android:background="@drawable/backgroundimg"
        android:text="Register Complain"
        android:textSize="20sp"
        app:backgroundTint="@null" />

    <EditText
        android:id="@+id/complain"
        android:layout_width="383dp"
        android:layout_height="100dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="234dp"
        android:ems="10"
        android:hint=" Enter your Complain here"
        android:inputType="textPersonName"
        android:textSize="20sp" />


</RelativeLayout>